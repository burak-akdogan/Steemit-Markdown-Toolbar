{
    "manifest_version": 3,
    "name": "Steemit Markdown Toolbar",
    "version": "1.0",
    "description": "Adds a markdown toolbar into the Steemit editor.",
    "content_scripts": [
      {
        "matches": ["https://steemit.com/*"],
        "js": ["content.js"],
        "css": ["content.css"]
      }
    ]
  }
  